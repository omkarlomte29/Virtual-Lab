{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "author": "Tanish Chavan",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "tsnd --respawn -r tsconfig-paths/register --pretty --transpile-only ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start:dist": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsnd -r tsconfig-paths/register --pretty --transpile-only scripts/seed2.ts",
    "db:migrate": "tsnd -r tsconfig-paths/register --pretty --transpile-only scripts/migrate.ts",
    "db:studio": "drizzle-kit studio --port 3306",
    "db:setup": "drizzle-kit push && ts-node scripts/seed2.ts"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@sinclair/typebox": "^0.33.7",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.1",
    "drizzle-typebox": "^0.1.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.3",
    "node-cache": "^5.1.2",
    "redis": "^4.7.0",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.23.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
